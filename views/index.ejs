<%- include('header') %>

<link rel="stylesheet" href="../public/css/style.css">
<div class="container-fluid">
    <div class="row">
       <% for (let i = 0; i < note.length ; i++) {%>
        <div class="card">
          <div class="card-body">
            <h5 class="card-title">Title:  <%= note[i]['title']%> </h5>
            <h5 class="card-title">Explanation:  <%= note[i]['explanation']%> 
            </h5>
            <div class="buttons">
              <a href="/edit/<%= note[i]['_id'] %>" class="btn btn-primary edit">Edit</a>
              <a href="/delete/<%= note[i]['_id'] %>" class="btn btn-danger ">Delete</a>
            </div>
          </div>
        </div>
      </div>
      <% } %>
        
    </div>
</div>
  
<%- include('footer') %>
  
